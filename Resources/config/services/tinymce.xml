<?xml version="1.0" ?>
<container xmlns="http://symfony.com/schema/dic/services"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://symfony.com/schema/dic/services http://symfony.com/schema/dic/services/services-1.0.xsd">
	
	<parameters>
		<parameter key="asf_layout.twig.tinymce.class">ASF\LayoutBundle\Twig\Extension\TinyMCEExtension</parameter>
	</parameters>
	 
	<services>
        <service id="asf_layout.twig.tinymce" class="%asf_layout.twig.tinymce.class%">
        	<argument>%asf_layout.assets%</argument>
            <tag name="twig.extension" />
        </service>
	</services>
	
</container>
