<?xml version="1.0" ?>
<container xmlns="http://symfony.com/schema/dic/services"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://symfony.com/schema/dic/services http://symfony.com/schema/dic/services/services-1.0.xsd">
	
	<parameters>
		<parameter key="asf_layout.twig.extension.assets.class">ASF\LayoutBundle\Twig\Extension\AssetsExtension</parameter>
	</parameters>
	
	<services>
		<service id="asf_layout.twig.extension.assets" class="%asf_layout.twig.extension.assets.class%">
			<argument>%asf_layout.assets%</argument>
			<argument>%asf_layout.enable_assetic_support%</argument>
			<tag name="twig.extension" />
		</service>
	</services>
	
</container>